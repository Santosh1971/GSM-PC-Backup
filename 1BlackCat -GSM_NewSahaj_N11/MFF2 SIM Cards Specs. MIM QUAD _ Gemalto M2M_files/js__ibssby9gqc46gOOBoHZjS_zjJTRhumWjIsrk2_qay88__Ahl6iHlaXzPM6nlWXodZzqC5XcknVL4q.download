(function ($) {

  Drupal.behaviors.gemaltoCookiePolicy = {
    attach: function (context, settings) {

      // Ensure that all variables are defined.
      settings.cookiePolicy = settings.cookiePolicy || false;
      var message = settings.cookiePolicy.message || false;
      var uid = settings.cookiePolicy.uid || 0;

      if (message) {

        // Generate message with "Hide" policy button.
        message = settings.cookiePolicy.message + '<a class="hide" href="#">' + Drupal.t('Hide', {}, {}) + '</a>';

        // Container for storing policy message.
        var $container = $('<div/>', {id: 'cookie-policy'});

        if (!$('#cookie-policy').length) {

          // Show message with anumation.
          $container.html(message).appendTo('body').animate({height: 58, padding: 7}, 500);

          // Add a click event to hide a cookie policy message.
          $('.hide', $container).click(function(e) {
            $container.animate({height: 0, padding: 0}, 500);

            if (uid !== 0) {
              $.post(settings.basePath + 'cookie_policy/' + uid);
            }
            else {
              $.cookie('hideCookiePolicy', 1, {
                path: Drupal.settings.basePath,
                // The cookie should "never" expire.
                expires: 36500
              });
            }

            e.preventDefault();
          });

        }

      }
    }
  }

})(jQuery);
